<!DOCTYPE html>
<html>
  <head>
    <title><%= title %></title>
    <link rel='stylesheet' href='/stylesheets/style.css' />
  </head>
  <body>
    <h1><%= title %></h1>
    <p>Welcome to <%= title %></p>
  
    <% messages.forEach(function(message){%>
      <p><strong><%=message.user%></strong> (<%=message.added%>)</p>
      <p><%=message.text%></p>
    <%}) %>


  </body>
</html>